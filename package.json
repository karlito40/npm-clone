{
	"name": "npm-clone",
	"displayName": "NPM Clone Package",
	"publisher": "karlito40",
	"description": "Git clone a npm package easily",
	"version": "0.0.3",
	"icon": "assets/icon.png",
	"engines": {
		"vscode": "^1.54.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/karlito40/npm-clone.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:npm-clone.cloneNpmPackage"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "npm-clone.cloneNpmPackage",
				"title": "Clone a NPM package"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.54.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
